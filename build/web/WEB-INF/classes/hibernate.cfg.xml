<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/travel_umroh_mihrab_qolbi_db</property>
    <property name="hibernate.connection.username">root</property>
    <mapping resource="Travel/Umroh/Mihrab/Qolbi/Model/DataJamaahModel.hbm.xml"/>
    <mapping resource="Travel/Umroh/Mihrab/Qolbi/Model/DataJadwalModel.hbm.xml"/>
    <mapping resource="Travel/Umroh/Mihrab/Qolbi/Model/DataPaketModel.hbm.xml"/>
    <mapping resource="Travel/Umroh/Mihrab/Qolbi/Model/DataPembayaranModel.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
